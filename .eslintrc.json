{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:require-path-exists/recommended",
    "prettier"
  ],
  "plugins": [
    "require-path-exists"
  ],
  "rules": {
    "no-console": 2
  }
}
